/* 		===== Class Eq a
							eq, sEq, dif, sDif 					:: a -> a -> Bool
*/

function eq (@x) { return 
function    (@y) { return 
							x == y
; }; }

function sEq (@x) { return 
function     (@y) { return 
							x === y
; }; }

function dif (@x) { return 
function     (@y) { return 
							x != y
; }; }

function sDif (@x) { return 
function      (@y) { return 
							x !== y
; }; }
